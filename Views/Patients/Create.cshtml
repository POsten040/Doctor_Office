@{
  Layout = "_Layout";
}

@model Doctor_Office.Models.Patient;

<h4>Add a Patient</h4>
@using (Html.BeginForm())
{
  @Html.LabelFor(model => model.Name)
  @Html.TextBoxFor(model => model.Name)
  @Html.LabelFor(model => model.Birthday)
  @Html.TextBoxFor(model => model.Birthday)
  <br><span class="glyphicon glyphicon-pencil" aria-hidden="true"></span><br><input type="submit" value="Add Patient">
}